generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id       Int       @id @default(autoincrement())
  title    String
  blog     String
  catagory String
  date     DateTime? @default(now())
  image    String?
  nickname String
}

enum Category {
  TECHNOLOGY
  FOOD
  TRAVEL
  EDUCATION
}
